{
  "expo": {
    "newArchEnabled": true,
    "plugins": ["expo-build-properties"],
    "android": {
      "buildProperties": {
        "android": {
          "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
          "gradleProperties": {
            "android.useAndroidX": "true",
            "android.enableJetifier": "true"
          }
        }
      }
    }
  }
}
